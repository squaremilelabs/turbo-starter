{
  "name": "turbo-starter",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:dev": "dotenv -e .env.dev.local turbo build",
    "lint": "turbo lint",
    "dev": "dotenv -e .env.dev.local -- turbo dev",
    "dev:prod": "dotenv -e .env.prod.local -- turbo dev",
    "db:generate": "turbo db:generate",
    "db:push:dev": "dotenv -e .env.dev.local -- turbo db:push",
    "db:push:accept:dev": "dotenv -e .env.dev.local -- turbo db:push:accept",
    "db:push:force:dev": "dotenv -e .env.dev.local -- turbo db:push:force",
    "db:push:prod": "dotenv -e .env.prod.local -- turbo db:push",
    "db:push:accept:prod": "dotenv -e .env.prod.local -- turbo db:push:accept",
    "db:push:force:prod": "dotenv -e .env.prod.local -- turbo db:push:force"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "turbo": "^2.2.3"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  }
}
