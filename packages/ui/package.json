  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/react": "^18.2.61",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
